{
  "name": "Todpole",
  "displayName": "Todpole",
  "privacy": true,
  "icon": "./src/assets/todpole_app_icon.png",
  "splash": {
    "image": "./src/assets/todpole_app_icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#84A59D"
  },
  "orientation": "portrait",
  "expo": {
    "ios": {
      "bundleIdentifier": "com.todpole-app.todpole",
      "buildNumber": "1.0.3",
      "icon": "./src/assets/todpole_app_icon.png",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Give Todpole access to the photo library to let the user upload his toddler activities ideas."
      }
    },
    "name": "Todpole",
    "slug": "Todpole",
    "version": "1.0.3",
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "description": ""
  }
}
